<!--pages/resourse/resourse.wxml-->
<navigation-bar title="OA" back="{{true}}" color="black" background="#FFF"></navigation-bar>
<scroll-view class="container" type="nested" scroll-y style="display: flex;flex-direction: column;">
  <view class="resourse-box" id="resourse-box">
    <view class="resourse-title">我的资源</view>
    <!-- 加载中元素 -->
    <view class="resourse-loading" wx:if="{{isLoading}}">
      Loading...
    </view>
    <!-- 列表 -->
    <view class="resourse-border">
      <scroll-view class="resourse-list" scroll-y="true" type="list" wx:if="{{!isLoading}}">
        <view class="resourse-item" wx:for="{{testarray}}">{{item}}</view>
      </scroll-view>
    </view>
  </view>
  <view class="resourse-box" id="resourse-box">
    <view class="resourse-title">社团资源</view>
    <!-- 加载中元素 -->
    <view class="resourse-loading" wx:if="{{isLoading}}">
      Loading...
    </view>
    <!-- 列表 -->
    <view class="resourse-border">
      <scroll-view class="resourse-list" scroll-y="true" type="list" wx:if="{{!isLoading}}">
        <view class="resourse-item" wx:for="{{testarray}}">{{item}}</view>
      </scroll-view>
    </view>
  </view>
</scroll-view>